{
  "name": "Employee",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "positionId": {
      "type": "number",
      "required": true
    },
    "departmentId": {
      "type": "number",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "ext": {
      "type": "string"
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "updatedAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "position": {
      "type": "belongsTo",
      "model": "Position",
      "foreignKey": "positionId"
    },
    "department": {
      "type": "belongsTo",
      "model": "Department",
      "foreignKey": "departmentId"
    }
  },
  "acls": [],
  "methods": {}
}
